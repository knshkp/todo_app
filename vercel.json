{
    "version":2,
    "builds":[
        {
        "src":"todo_app/wsgi.py",
        "use":"@vercel/python",
        "config":{"maxlambdasize":"15mb","runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{"dist_dir":"staticfiles_build"}            
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"todo_app/wsgi.py"
        }
    ]
}